#include <bits/stdc++.h>
using namespace std;
 void reverseArray(int arr[],  int n){
 	int i=0;
 	int j=n-1;
 	
 while(i<=j){
 		swap(arr[i],arr[j]);
 //	arr[i]=arr[j];
 		i++;
 		j--;
	 }
 } 
  
  
  
  
int main(){
	int arr[]={3,5,7,6,9,2,5};
	reverseArray( arr,7);
	//Print the array 
	for(int j=0;j<7;j++){
		cout <<arr[j]<<"    ";
	}
//	cout <<endl;

	
	
	
	
	return 0;
}
